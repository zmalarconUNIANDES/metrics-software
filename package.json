{
  "name": "front-metrics",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:es": "ng serve --configuration=es",
    "start:en": "ng serve --configuration=en",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "locale": "ng xi18n --output-path src/locale",
    "sonar": "sonar-scanner",
    "coverage": "ng test --watch --code-coverage",
    "prettier": "prettier --check 'src/app/**/*.ts'",
    "prettier:write": "prettier --write '{e2e,test-helpers,src}/**/*.ts'",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
    "lint:html": "htmlhint src/**/*.html",
    "lint:scss": "stylelint src/app/**/**/*.scss --syntax scss"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.5",
    "@angular/common": "~10.0.5",
    "@angular/compiler": "~10.0.5",
    "@angular/core": "~10.0.5",
    "@angular/forms": "~10.0.5",
    "@angular/localize": "^12.0.0",
    "@angular/platform-browser": "~10.0.5",
    "@angular/platform-browser-dynamic": "~10.0.5",
    "@angular/router": "~10.0.5",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.4.1",
    "exec-sh": "^0.3.4",
    "faker": "^5.5.3",
    "jquery": "^3.6.0",
    "ngx-toastr": "^13.2.1",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "url-template": "^2.0.8",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular/cli": "~10.0.4",
    "@angular/compiler-cli": "~10.0.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "htmlhint": "^0.11.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sonarqube-unit-reporter": "0.0.23",
    "prettier": "^2.2.1",
    "prettier-stylelint": "^0.4.2",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "sonar-scanner": "^3.1.0",
    "stylelint": "^13.13.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-scss": "^3.19.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-clean-code": "^0.2.10",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run prettier && npm run lint:html"
    }
  }
}
